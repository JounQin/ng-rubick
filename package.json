{
  "name": "ng-rubick",
  "private": true,
  "version": "0.0.1",
  "description": "Refactor the whole Rubick project with better structure and AOT compiler",
  "repository": "git@github.com:JounQin/ng-rubick.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd .env env-cmd --fallback .env.local ts-node server",
    "build": "rimraf dist && env-cmd --fallback .env.local env-cmd .env webpack --config build/webpack.config.ts --colors --progress",
    "start": "env-cmd --fallback .env.local env-cmd .env ts-node server",
    "lint": "tslint -p . -t stylish '**/*.ts'",
    "precommit": "lint-staged",
    "template:build": "pug src -P --doctype html",
    "template:clean": "rimraf src/**/*.html",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh"
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "tslint -p . -t stylish",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": null
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "*.i18n.json",
        "options": {
          "printWidth": 1000
        }
      }
    ]
  },
  "dependencies": {
    "@angular/common": "^6.0.5",
    "@angular/core": "^6.0.5",
    "@angular/forms": "^6.0.5",
    "@angular/platform-browser": "^6.0.5",
    "@angular/platform-browser-dynamic": "^6.0.5",
    "@angular/router": "^6.0.5",
    "debug": "^3.1.0",
    "env-cmd": "^8.0.2",
    "koa": "^2.5.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-static-cache": "^5.1.2",
    "lodash-es": "^4.17.10",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/compiler": "^6.0.5",
    "@angular/compiler-cli": "^6.0.5",
    "@angular/language-service": "^6.0.5",
    "@ngtools/webpack": "^6.0.8",
    "@types/debug": "^0.0.30",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/koa": "^2.0.46",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-logger": "^3.1.0",
    "@types/koa-static-cache": "^5.1.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.110",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.1.4",
    "@types/source-map": "0.5.2",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.1",
    "autoprefixer": "^9.0.0",
    "css-loader": "^0.28.11",
    "exports-loader": "^0.7.0",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-alpha",
    "husky": "^0.14.3",
    "koa-webpack": "^5.0.2",
    "lint-staged": "^7.2.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.5",
    "prettier": "^1.13.5",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "sass-resources-loader": "^1.3.3",
    "style-loader": "^0.23.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8"
  }
}
