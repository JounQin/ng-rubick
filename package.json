{
  "name": "ng-rubick",
  "private": true,
  "version": "0.0.1",
  "description": "Refactor the whole Rubick project with better structure and AOT compiler",
  "repository": "git@github.com:JounQin/ng-rubick.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd .prod.env env-cmd .dev.env ts-node server",
    "build": "rimraf dist && env-cmd .dev.env env-cmd .prod.env webpack --config build/webpack.config.ts --colors --progress",
    "start": "env-cmd .dev.env env-cmd .prod.env ts-node server",
    "lint": "tslint -p . -t stylish",
    "template:build": "pug src -P --doctype html",
    "template:clean": "rimraf src/**/*.html",
    "sync:pull": "bash build/sync-pull.sh",
    "sync:push": "bash build/sync-push.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "tslint -p . -t stylish",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@angular/common": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "debug": "^4.1.1",
    "env-cmd": "^8.0.2",
    "koa": "^2.7.0",
    "koa-compose": "^4.1.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-static-cache": "^5.1.2",
    "lodash-es": "^4.17.11",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular/compiler": "^7.2.3",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@ngtools/webpack": "^7.3.0",
    "@types/debug": "^4.1.0",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/koa": "^2.0.48",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-compress": "^2.0.8",
    "@types/koa-logger": "^3.1.1",
    "@types/koa-static-cache": "^5.1.0",
    "@types/koa-webpack": "^5.0.1",
    "@types/lodash": "^4.14.120",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^10.12.20",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.24",
    "autoprefixer": "^9.4.7",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "exports-loader": "^0.7.0",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "koa-webpack": "^5.2.1",
    "lint-staged": "^8.1.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.3",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "pug-plain-loader": "^1.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-angular": "^1.1.2",
    "tslint-config-prettier": "^1.17.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1"
  }
}
